[run]
source = .
omit =
    tests/*
    venv/*
    */__pycache__/*
    */site-packages/*
    */node_modules/*
    mcp_server.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

